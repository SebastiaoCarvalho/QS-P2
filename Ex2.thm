<?xml version="1.0"?>
<alloy>

<view>

<defaultnode/>

<defaultedge/>

<node>
   <type name="Int"/>
   <type name="String"/>
   <type name="univ"/>
   <type name="seq/Int"/>
</node>

<node color="White" label="PendingMsg">
   <type name="Ex1/PendingMsg"/>
</node>

<node label="Msg">
   <type name="Ex1/Msg"/>
</node>

<node label="Node">
   <type name="Ex1/Node"/>
</node>

<node label="SendingMsg">
   <type name="Ex1/SendingMsg"/>
</node>

<node label="SentMsg">
   <type name="Ex1/SentMsg"/>
</node>

<node showlabel="no">
   <set name="Ex1/LQueue" type="Ex1/Node"/>
</node>

<node showlabel="no" color="Blue">
   <set name="Ex1/Member" type="Ex1/Node"/>
</node>

<node showlabel="no" color="Green">
   <set name="Ex1/Leader" type="Ex1/Node"/>
</node>

<node visible="yes" showlabel="no">
   <set name="$msg" type="Ex1/Msg"/>
</node>

<edge visible="no">
   <relation name="lnxt"> <type name="Ex1/Node"/> <type name="Ex1/Node"/> <type name="Ex1/Node"/> </relation>
   <relation name="qnxt"> <type name="Ex1/Node"/> <type name="Ex1/Node"/> <type name="Ex1/Node"/> </relation>
</edge>

</view>

</alloy>
